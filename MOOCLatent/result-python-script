filenames = ['results-disengagement0.val', 'results-disengagement1.val', 'results-disengagement2.val', 'results-disengagement3.val','results-disengagement4.val','results-disengagement5.val','results-disengagement6.val','results-disengagement7.val','results-disengagement8.val','results-disengagement9.val']
with open('/linqshomes/artir/Doctorate/workspace/MOOCLatent/data/t3/output/disengagement.txt', 'w') as outfile:
    for fname in filenames:
        with open(fname) as infile:
            for line in infile:
                outfile.write(line)

